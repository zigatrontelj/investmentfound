```{r}
realpersonalconsumptionexpenditures <- Quandl('FRED/DPCERA3M086SBEA', start_date=SDate, end_date=EDate, collapse = "monthly") %>% 
  tq_transmute(select = Value, mutate_fun = to.monthly, indexAt = "firstof") %>% tq_transmute(select = Value,         
               mutate_fun = periodReturn,   
               period = "monthly",      
               col_rename = "Value") 

realpersonalconsumptionexpenditures %>% 
  ggplot(aes(Date, Value)) + 
  geom_line() +
  scale_x_date(name = "Year", date_minor_breaks = "10 year") + 
  scale_y_continuous(name = "N° of building permits") +
  ggtitle('Real personal Consumption Expenditure')+
  annotate("rect", xmin = as.Date("2020/02/01"), xmax = as.Date("2020/04/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple') +
  annotate("rect", xmin = as.Date("2007/12/01"), xmax = as.Date("2009/06/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple') +
