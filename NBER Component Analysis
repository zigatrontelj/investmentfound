ProducerPriceIndex <- Quandl('FRED/PPIACO', start_date=SDate, end_date=EDate, collapse = "monthly") %>% 
  tq_transmute(select = Value, mutate_fun = to.monthly, indexAt = "firstof") %>% tq_transmute(select = Value,         
               mutate_fun = periodReturn,   
               period = "monthly",      
               col_rename = "Value") 

