```{r}
employmentrate <- Quandl('FRED/EMRATIO', start_date=SDate, end_date=EDate, collapse = "monthly") %>% 
  tq_transmute(select = Value, mutate_fun = to.monthly, indexAt = "firstof") %>% tq_transmute(select = Value,         mutate_fun = periodReturn,   period = "monthly",      col_rename = "Value") 

employmentrate$Value <- employmentrate$Value/100
employmentrate %>% 
  ggplot(aes(Date, Value)) + 
  geom_line() +
  scale_x_date(name = "Year", date_minor_breaks = "10 year") + 
  scale_y_continuous(name = "N° of building permits") +
  ggtitle('Employment Rate')+
  annotate("rect", xmin = as.Date("2020/02/01"), xmax = as.Date("2020/04/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple') +
  annotate("rect", xmin = as.Date("2007/12/01"), xmax = as.Date("2009/06/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple') +
  annotate("rect", xmin = as.Date("2001/03/01"), xmax = as.Date("2001/11/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple') +
  annotate("rect", xmin = as.Date("1990/07/01"), xmax = as.Date("1991/03/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')+
  annotate("rect", xmin = as.Date("1981/07/01"), xmax = as.Date("1982/11/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')+
  annotate("rect", xmin = as.Date("1980/01/01"), xmax = as.Date("1980/07/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')+
  annotate("rect", xmin = as.Date("1973/11/01"), xmax = as.Date("1975/05/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')+
  annotate("rect", xmin = as.Date("1969/12/01"), xmax = as.Date("1970/11/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')+
  annotate("rect", xmin = as.Date("1960/04/01"), xmax = as.Date("1961/02/01"), ymin = -1, ymax = 1, alpha = .2, fill = 'purple')
